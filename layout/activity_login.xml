<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".login">

    <FrameLayout
        android:id="@+id/fl_fragment_login_register"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="#14213d"
        app:layout_constraintBottom_toTopOf="@+id/btm_nav_user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        app:menu="@menu/nav_bar_login_reg"
        android:id="@+id/btm_nav_user"
        android:layout_width="match_parent"
        android:background="#14213d"
        android:layout_height="75dp"
        app:itemIconTint="#646262"
        app:itemTextColor="#646262"
        app:itemBackground="@color/btn_nav_color_selcotr"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>